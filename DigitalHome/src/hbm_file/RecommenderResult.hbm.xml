<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.whieb.digitalhome.model.RecommenderResult"
		table="recommender_result">
		<id column="id" name="id" type="long">
			<generator class="native" />
		</id>
		<property name="userId" column="userId" />
		<property name="itemId" column="itemId" />
		<property name="value" column="value" />
		<property name="type" column="type" />
		<property name="date" column="date" />
		<property name="predictor_name" column="predictor_name" />
		<property name="explain" column="explain" />
	</class>
</hibernate-mapping>