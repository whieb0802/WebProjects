<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.whieb.digitalhome.model.SourceItem" table="source_item">
    <id column="id" name="id" type="long">
    </id>
    <property column="name" name="name"/>
    <property column="cpname" name="cpname"/>
    <property column="playtimes" name="playtimes"/>
    <property column="num" name="num"/>
    <property column="avetimes" name="avetimes"/>
    <property column="fee" name="fee"></property>
  </class>
</hibernate-mapping>