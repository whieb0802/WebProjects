<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.whieb.digitalhome.model.Trade" table="trade">
    <id column="id" name="id" type="long">
      <generator class="native"/>
    </id>
    <property column="userId" name="userId"/>
    <property column="itemId" name="itemId"/>
    <property column="date" name="date" type="timestamp"/>
    <property column="totalPrice" name="totalPrice"/>
  </class>
</hibernate-mapping>